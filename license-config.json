{
  "ignore": [
    ".claspignore",
    ".editorconfig",
    ".eslintignore",
    ".github",
    ".gitignore",
    ".prettierignore",
    "*.txt",
    "**/.editorconfig",
    "**/.gitignore",
    "**/*.test.ts",
    "**/README.md",
    "build",
    "CONTRIBUTING",
    "dist",
    "README.md",
    "ui/src/*.d.ts",
    "ui/src/*.html",
    "ui/src/*.scss",
    "ui/src/**/*.config.ts",
    "ui/src/**/*.html",
    "ui/src/**/*.scss"
  ],
  "license": "license-header.txt",
  "licenseFormats": {
    "js|ts|mjs": {
      "prepend": "/**",
      "append": " */",
      "eachLine": {
        "prepend": " * "
      }
    },
    "html|md": {
      "prepend": "<!--",
      "append": "-->"
    },
    "sh|^pre-commit": {
      "eachLine": {
        "prepend": "# "
      }
    }
  },
  "trailingWhitespace": "TRIM"
}
